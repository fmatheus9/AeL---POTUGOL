programa
{
	inclua biblioteca Util --> u
	
	funcao inicio()
	{
		inteiro mat[5][5],i,j, gaveta
		const inteiro TAM = 0

		para(i=0;i<5;i++){
			para(j=0;j<5;j++){
				//escreva("Informe os valores da matriz: ")
				//leia(mat[i][j])
				mat[i][j] = u.sorteia(10, 99)
			}
		}
		escreva("Matriz Principal\n")
		para(i=0;i<5;i++){
			para(j=0;j<5;j++){
				escreva(mat[i][j]," ")
			}
			escreva("\n")
		}
		para(i=0;i<5;i++){
			para(j=1;j<2;j++){
				gaveta = mat[i][j]
				mat[i][j] = mat[i][j+3]
				mat[i][j+3] = gaveta
			}
		}
		escreva("\nMatriz Trocada\n")
		para(i=0;i<5;i++){
			para(j=0;j<5;j++){
				escreva(mat[i][j]," ")
			}
			escreva("\n")
		}
	}
}

