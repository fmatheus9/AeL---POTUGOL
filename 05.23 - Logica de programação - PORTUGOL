programa {
  funcao inicio() {
    inteiro vet[10], i
    para(i=0;i<10;i++){
      escreva("Valor ",i+1,":")
      leia(vet[i])
    }
    para(i=0;i<10;i++){
        escreva(vet[i]*3,"\n")
    }
  }
}

