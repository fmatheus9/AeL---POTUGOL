programa
{
	
	funcao inicio()
	{
		caracter mam, ave, rep, quad, bip, voa, aqua, car, herb, oni, frut
		escreva("É mamífero? (s ou n) ")
		leia(mam)
		escolha (mam)
		{
		caso 's':
		escreva("É quadrupede? ")
		leia(quad)
			escolha (quad){
				caso's':
				escreva("É carnívoro? ")
				leia(car)
				se (car == 's'){
					escreva("O animal escolhido foi o LEÃO")
					}
				senao{
					escreva("É herbívoro? ")
					leia(herb)
					se (herb == 's'){
						escreva("O animal escolhido foi o CAVALO")
						}
					senao{
						escreva("Animal não cadastrado no dados")
						}
					}
				pare
				
				caso 'n':
				escreva("É bípede? ")
				leia(bip)
					escolha (bip){
						caso 's':
						escreva("É onívoro? ")
						leia(oni)
						se (oni == 's'){
							escreva("O animal escolhido foi o HOMEM")
							}
						senao{
							escreva("É frutífero? ")
							leia(frut)
							se (frut == 's'){
								escreva("O animal escolhido foi o MACACO")
								}
							senao{
								escreva("Animal não cadastrado no dados")
								}
							}
						}
						}
				pare
				
		
		caso 'n':
		escreva("É uma ave? (s ou n)")
		leia(ave)
			escolha (ave){
				
				}
		
		}
		
		 
		
	}
}
