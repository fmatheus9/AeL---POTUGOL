  //MATHEUS FERRAREZI MESQUITA FAGUNDES//
  
	funcao inicio()
	{
		real n1, n2, n3, A, B, C
		escreva("Informe o valor 1: ")
		leia(A)
		escreva("Informe o valor 2: ")
		leia(B)
		escreva("Informe o valor 3: ")
		leia(C)
		se (A> B e A> C){
			se (A >= B+C  ){
				escreva("Nenhum triângulo é formado")
			}
			senao{
			se (A*A == B*B+C*C){
				escreva("Um triângulo retângulo é formado")
			}
			se (A*A > B*B+C*C ){
				escreva("Um triângulo obtusângulo é formado")
			}
			se (A*A < B*B+C*C){
				escreva("Um triângulo acutângulo é formado")
			}
			}
		}	

		se (B> A e B> C){
			se (B >= A+C ){
				escreva("Nenhum triângulo é formado")
			}
			senao{
			se (B*B == A*A+C*C){
				escreva("Um triângulo retângulo é formado")
			}
			se (B*B > A*A+C*C ){
				escreva("Um triângulo obtusângulo é formado")
			}
			se (B*B < A*A+C*C){
				escreva("Um triângulo acutângulo é formado")
			}
			}
		}

		se (C> A e C> B){
			se (C >= B+A ){
				escreva("Nenhum triângulo é formado")
			}
			senao{
			se (C*C == B*B+A*A){
				escreva("Um triângulo retângulo é formado")
			}
			se (C*C > B*B+A*A){
				escreva("Um triângulo obtusângulo é formado")
			}
			se (C*C < B*B+A*A){
				escreva("Um triângulo acutângulo é formado")
			}
			}
			
		}
		
	}
